{
  "manifest_version": 2,
  "name": "Simple FB Messenger",
  "version": "1.3",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "/images/icon_16.png",
    "128": "/images/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "webview"
  ]
}
